      DTS:Expression="&quot;DELETE A&#xA;  FROM dbo.FACT_PURCHASE_METRIC A&#xA; WHERE &#xA;     ( [USER_NAME] LIKE '%test%'&#xA;    OR [USER_NAME] LIKE '%buat%'&#xA;    OR [USER_NAME] LIKE '%install%'&#xA;    OR [USER_NAME] LIKE '%ch3ck%'&#xA;    OR [USER_NAME] LIKE '%bu4t%'&#xA;    OR [USER_NAME] LIKE '%1nst4ll%'&#xA;    OR [USER_NAME] LIKE '%t3st%'&#xA;    OR [USER_NAME] LIKE '%check%'&#xA;    OR [USER_NAME] LIKE '%t4st%'&#xA;    OR [USER_NAME] LIKE '%MDS00%'&#xA;&#x9; )&#xA;   AND NOT EXISTS &#xA;     (&#xA;&#x9;   SELECT 1&#xA;&#x9;     FROM &quot; + @[$Project::strLinkedServerEDWtoSTG] + &quot;bre.STAGE_USER_DISPOSITION B&#xA;&#x9;&#x9;WHERE B.DISPOSITION = 0 &#xA;&#x9;&#x9;  AND B.USER_NAME = A.USER_NAME &#xA;     )&#xA;&#xA;&quot;"
      DTS:IncludeInDebugDump="2345"
      DTS:Namespace="User"
      DTS:ObjectName="strDeleteTestSQL">
      <DTS:VariableValue
        DTS:DataType="8">DELETE A
  FROM dbo.FACT_PURCHASE_METRIC A
 WHERE 
     ( [USER_NAME] LIKE '%test%'
    OR [USER_NAME] LIKE '%buat%'
    OR [USER_NAME] LIKE '%install%'
    OR [USER_NAME] LIKE '%ch3ck%'
    OR [USER_NAME] LIKE '%bu4t%'
    OR [USER_NAME] LIKE '%1nst4ll%'
    OR [USER_NAME] LIKE '%t3st%'
    OR [USER_NAME] LIKE '%check%'
    OR [USER_NAME] LIKE '%t4st%'
    OR [USER_NAME] LIKE '%MDS00%'
	 )
   AND NOT EXISTS 
     (
	   SELECT 1
	     FROM ODS_ALL_RW.ODS_STAGING.bre.STAGE_USER_DISPOSITION B
		WHERE B.DISPOSITION = 0 
		  AND B.USER_NAME = A.USER_NAME 
     )

</DTS:VariableValue>
