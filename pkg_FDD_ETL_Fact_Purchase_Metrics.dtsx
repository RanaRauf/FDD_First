              SQLTask:Connection="{298443CF-8E9E-4DB0-8927-09AAF4FDC24A}"
              SQLTask:BypassPrepare="False"
              SQLTask:SqlStatementSource="--get initial dataset&#xA;IF EXISTS(SELECT * FROM sys.objects WHERE object_id = OBJECT_ID(N'[dbo].[tmpPurchaseMetric]') AND type in (N'U'))&#xA;&#x9;DROP TABLE tmpPurchaseMetric;&#xA;&#xA;WITH fact_purchase_metric_row_num AS&#xA;&#x9;(&#xA;&#x9;&#x9;SELECT product_id, customer_id, purchase_date_id, &#xA;&#x9;&#x9;&#x9;&#x9;ROW_NUMBER() OVER (PARTITION BY product_id ORDER BY purchase_date_id asc) AS row_num&#xA;&#x9;&#x9;FROM fact_purchase_metric&#xA;&#x9;&#x9;WHERE TRANSACTION_ID LIKE 'MSP%'&#xA;&#x9;&#x9;&#x9;AND CUSTOMER_ID &gt; 0&#x9;--do not include -1 customer_IDs as this distorts renewal count&#xA;&#x9;&#x9;--DONT WE NEED TO CONSTRAIN THIS BASED ON CERTAIN DATES? We don't want to run against everything, do we? &#xA;&#x9;&#x9;--WHERE PURCHASE_DATE_ID &gt;= DATEADD(d,-50, @somedate)&#xA;&#x9;&#x9;--&#x9;AND TRANSACTION_ID LIKE 'MSP%'&#xA;&#x9;)&#xA;&#x9;select a.product_id, a.customer_id, a.purchase_date_id &#xA;&#x9;&#x9;INTO tmpPurchaseMetric&#xA;&#x9;&#x9;from fact_purchase_metric_row_num  a&#xA;&#x9;&#x9;&#x9;left outer join fact_purchase_metric_row_num  b&#xA;&#x9;&#x9;&#x9;&#x9;on a.product_id = b.product_id &#xA;&#x9;&#x9;&#x9;&#x9;&#x9;and a.customer_id = b.CUSTOMER_ID&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;and a.row_num = b.row_num + 1&#xA;&#x9;&#x9;where abs(a.purchase_date_id - b.purchase_date_id ) &lt; 46  --hard coded to 45 days&#xA;&#x9;&#x9;order by a.customer_id, a.product_id&#x9;&#xA;&#xA;--update renewal count&#xA;UPDATE a&#xA; SET RENEWAL_PURCHASE_CNT = 1&#xA; FROM [dbo].[FACT_PURCHASE_METRIC] a&#xA; JOIN tmpPurchaseMetric b ON a.CUSTOMER_ID = b.CUSTOMER_ID&#xA;&#x9;&#x9;AND a.PRODUCT_ID = b.PRODUCT_ID&#xA;&#x9;&#x9;AND a.PURCHASE_DATE_ID = b.PURCHASE_DATE_ID &#xA; JOIN EDW2.dbo.DIM_DATE d ON a.PURCHASE_DATE_ID = d.DATE_ID &#xA; WHERE d.DATE &lt; '2013-05-01'&#x9;--hard coded to May 1&#xA;&#xA;IF EXISTS(SELECT * FROM sys.objects WHERE object_id = OBJECT_ID(N'[dbo].[tmpPurchaseMetric]') AND type in (N'U'))&#xA;&#x9;DROP TABLE tmpPurchaseMetric&#xA;" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
