                    <property
                      dataType="System.String"
                      description="The SQL command to be executed."
                      name="SqlCommand"
                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=11.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">select f.PURCHASE_DATE_ID, f.FLIGHT_ID, f.AIRCRAFT_ID, f.PRODUCT_ID, sum(f.TOTAL_AMT) as AMOUNT
,(case when f.RENEWAL_PURCHASE_CNT&lt;&gt; 0 then 'MSP Renewal'
      when f.RENEWAL_PURCHASE_CNT= 0 and d.CHANNEL = 'Air' and d.PRODUCT_NAME = 'IFC' then 'Regular - Air'
	  when f.RENEWAL_PURCHASE_CNT= 0 and d.CHANNEL = 'Ground' then 'Regular - Ground'
