          DTS:DelayValidation="True"
          DTS:Description="Execute SQL Task"
          DTS:DTSID="{E011C752-6363-425D-ACD9-AF6BAAE3C410}"
          DTS:ExecutableType="Microsoft.SqlServer.Dts.Tasks.ExecuteSQLTask.ExecuteSQLTask, Microsoft.SqlServer.SQLTask, Version=11.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91"
          DTS:LocaleID="-1"
          DTS:ObjectName="SQL - Update from Temp"
          DTS:TaskContact="Execute SQL Task; Microsoft Corporation; SQL Server 2012; © 2007 Microsoft Corporation; All Rights Reserved;http://www.microsoft.com/sql/support/default.asp;1"
          DTS:ThreadHint="0">
          <DTS:Variables />
          <DTS:ObjectData>
            <SQLTask:SqlTaskData
              SQLTask:Connection="{298443CF-8E9E-4DB0-8927-09AAF4FDC24A}"
              SQLTask:SqlStatementSource="USE EDW2&#xA;&#xA;IF OBJECT_ID('tempdb..#FSR_TEMP') IS NOT NULL DROP TABLE #FSR_TEMP&#xA;CREATE TABLE #FSR_TEMP&#xA;(&#xA;AIRLINE_ID NUMERIC(38,0),&#xA;DATE_ID NUMERIC(38,0),&#xA;REVENUE_ID NUMERIC(38,0),&#xA;REVENUE_AMT NUMERIC(11,2),&#xA;PRODUCT_ID NUMERIC(38,0),&#xA;EDW_INSERT_SOURCE NVARCHAR(50), &#xA;EDW_UPDATE_SOURCE NVARCHAR(50), &#xA;REGIONALITY VARCHAR(10)&#xA;)&#xA;&#xA;UPDATE A&#xA;   SET A.REVENUE_AMT = B.REVENUE_AMT&#xA;  FROM dbo.FACT_SALES_REVENUE A&#xA;  JOIN ##FACT_SALES_REVENUE B&#xA;    ON A.AIRLINE_ID = B.AIRLINE_ID&#xA;   AND A.DATE_ID = B.DATE_ID&#xA;   AND A.PRODUCT_ID = B.PRODUCT_ID&#xA;   AND A.REVENUE_ID = B.REVENUE_ID&#xA;   AND A.REGIONALITY = B.REGIONALITY&#xA;JOIN dbo.DIM_SALES_REVENUE C&#xA;  ON A.REVENUE_ID = C.REVENUE_ID &#xA;WHERE C.REVENUE_SOURCE NOT IN ('MSP Renewal', 'Regular - Air', 'Regular - Ground', 'Video')&#xA;&#xA;DELETE FROM ##FACT_SALES_REVENUE WHERE REVENUE_ID NOT IN &#xA;(&#xA;SELECT REVENUE_ID&#xA;  FROM dbo.DIM_SALES_REVENUE&#xA; WHERE REVENUE_SOURCE IN ('MSP Renewal', 'Regular - Air', 'Regular - Ground', 'Video') -- , 'IPass', 'Boingo')&#xA;)&#xA;&#xA;ALTER TABLE ##FACT_SALES_REVENUE ADD IDC INT IDENTITY(1,1)&#xA;GO&#xA;&#xA;--select * from ##FACT_SALES_REVENUE&#xA;&#xA;CREATE INDEX IDX_TEMP_SR ON ##FACT_SALES_REVENUE (IDC)&#xA;&#xA;DECLARE @EXIT_FLAG AS CHAR(1)&#xA;DECLARE @CNT AS INT &#xA;DECLARE @IDC AS INT &#xA;&#xA;DECLARE @AIRLINE_ID  AS NUMERIC(38,0)&#xA;DECLARE @DATE_ID     AS NUMERIC(38,0)&#xA;DECLARE @REVENUE_ID  AS NUMERIC(38,0)&#xA;DECLARE @PRODUCT_ID  AS NUMERIC(38,0)&#xA;DECLARE @REVENUE_AMT AS NUMERIC(11,2)&#xA;DECLARE @REGIONALITY AS VARCHAR(10)&#xA;&#xA;SET @EXIT_FLAG = '1'&#xA;&#xA;SELECT @CNT = COUNT(1) FROM ##FACT_SALES_REVENUE&#xA;&#xA;IF @CNT &gt; 0 &#xA;BEGIN &#xA;  SET @EXIT_FLAG = '0'&#xA;END&#xA;&#xA;SET @IDC = 1 &#xA;&#xA;WHILE @EXIT_FLAG = '0' &#xA;BEGIN&#xA;&#xA;--print CAST(@IDC AS VARCHAR(10))&#xA;&#xA;PRINT 'LOOP SELECT'&#xA;&#xA;    SELECT @PRODUCT_ID = PRODUCT_ID,      @AIRLINE_ID = AIRLINE_ID, &#xA;&#x9;       @DATE_ID = DATE_ID,            @REVENUE_ID = REVENUE_ID, &#xA;&#x9;&#x9;   @REVENUE_AMT = REVENUE_AMT,    @REGIONALITY = REGIONALITY &#xA;&#x9;  FROM ##FACT_SALES_REVENUE &#xA;&#x9; WHERE IDC = @IDC &#xA;&#xA;PRINT 'DELETE FROM TST TABLE'&#xA;Print @PRODUCT_ID&#xA;PRINT @AIRLINE_ID&#xA;print @DATE_ID&#xA;print @REVENUE_ID&#xA;print @REGIONALITY&#xA;&#xA;  --  DELETE &#xA;&#x9; -- FROM dbo.FACT_SALES_REVENUE&#xA;&#x9; --WHERE PRODUCT_ID = @PRODUCT_ID  &#xA;&#x9; --  AND AIRLINE_ID = @AIRLINE_ID&#xA;&#x9; --  AND DATE_ID    = @DATE_ID &#xA;&#x9; --  AND REVENUE_ID = @REVENUE_ID&#xA;&#x9; --  AND REGIONALITY = ISNULL(@REGIONALITY, 'US-US')&#xA;&#x9;   -- add revenue id filter&#xA;&#xA;--PRINT 'RE-INSERT'&#xA;--    INSERT INTO dbo.FACT_SALES_REVENUE (AIRLINE_ID, DATE_ID, REVENUE_ID, REVENUE_AMT, PRODUCT_ID, EDW_INSERT_SOURCE, EDW_UPDATE_SOURCE, REGIONALITY)&#xA;&#xA;    INSERT INTO #FSR_TEMP (AIRLINE_ID, DATE_ID, REVENUE_ID, REVENUE_AMT, PRODUCT_ID, EDW_INSERT_SOURCE, EDW_UPDATE_SOURCE, REGIONALITY)&#xA;&#x9;SELECT X.NEW_AIRLINE_ID, PURCHASE_DATE_ID AS DATE_ID, SR.REVENUE_ID, SUM(AMOUNT) AS REVENUE_AMT, PRODUCT_ID, 'pkg_FDD_ETL_EDW_Fact_Sales_Revenue.dtsx', 'pkg_FDD_ETL_EDW_Fact_Sales_Revenue.dtsx', ISNULL(DF.REGIONALITY, 'US-US') AS REGIONALITY&#xA;&#x9;  FROM &#xA;&#x9;(&#xA;&#x9;select f.PURCHASE_DATE_ID, f.FLIGHT_ID, f.AIRCRAFT_ID, f.PRODUCT_ID, sum(f.TOTAL_AMT) as AMOUNT, &#xA;&#x9;&#x9;   (&#xA;&#x9;&#x9;   case &#xA;&#x9;&#x9;   when f.RENEWAL_PURCHASE_CNT&lt;&gt; 0 then 'MSP Renewal'&#xA;&#x9;&#x9;   when f.RENEWAL_PURCHASE_CNT= 0 and d.CHANNEL = 'Air' and d.PRODUCT_NAME = 'IFC' then 'Regular - Air'&#xA;&#x9;&#x9;   when f.RENEWAL_PURCHASE_CNT= 0 and d.CHANNEL = 'Ground' then 'Regular - Ground'&#xA;&#x9;&#x9;   when d.PRODUCT_NAME = 'IFE' then 'Video'&#xA;&#x9;&#x9;   end&#xA;&#x9;&#x9;   ) as REVENUE_SOURCE ,sum(f.PURCHASE_CNT) as Purchase_Count, f.AIRLINE_ID AS NEW_AIRLINE_ID&#xA;&#x9;  from dbo.FACT_PURCHASE_METRIC f&#xA;&#x9;  LEFT join DIM_PRODUCT d on f.PRODUCT_ID = d.PRODUCT_ID&#xA;&#x9; WHERE f.AIRLINE_ID = @AIRLINE_ID &#xA;&#x9;   AND f.PRODUCT_ID = @PRODUCT_ID &#xA;&#x9;   AND f.PURCHASE_DATE_ID = @DATE_ID &#xA;&#x9;   -- add revenue id filter&#xA;&#x9; group by f.PURCHASE_DATE_ID, f.FLIGHT_ID, F.AIRCRAFT_ID, f.PRODUCT_ID,f.RENEWAL_PURCHASE_CNT,d.CHANNEL,&#xA;&#x9;&#x9;   d.PRODUCT_NAME, f.AIRLINE_ID&#xA;&#x9;) X &#xA;&#x9;  JOIN dbo.DIM_SALES_REVENUE SR ON X.REVENUE_SOURCE = SR.REVENUE_SOURCE &#xA;&#x9;LEFT OUTER JOIN dbo.DIM_FLIGHT DF ON X.FLIGHT_ID = DF.FLIGHT_ID&#xA;&#x9; WHERE SR.REVENUE_ID = @REVENUE_ID&#xA;&#x9; --AND DF.REGIONALITY = @REGIONALITY &#xA;&#x9;   -- add revenue id filter&#xA;&#x9; GROUP BY PURCHASE_DATE_ID, PRODUCT_ID, SR.REVENUE_ID, X.NEW_AIRLINE_ID, ISNULL(DF.REGIONALITY, 'US-US')&#xA;&#x9; ORDER BY PURCHASE_DATE_ID, PRODUCT_ID, SR.REVENUE_ID, X.NEW_AIRLINE_ID--, DF.REGIONALITY&#xA;&#xA;&#x9;DELETE&#x9;A&#xA;&#x9;FROM&#x9;dbo.FACT_SALES_REVENUE A&#xA;&#x9;JOIN&#x9;#FSR_TEMP B &#xA;&#x9;ON&#x9;&#x9;A.AIRLINE_ID = B.AIRLINE_ID&#xA;&#x9;AND&#x9;&#x9;A.DATE_ID = B.DATE_ID&#xA;&#x9;AND&#x9;&#x9;A.REVENUE_ID = B.REVENUE_ID &#xA;&#x9;AND&#x9;&#x9;A.PRODUCT_ID = B.PRODUCT_ID&#xA;&#x9;AND&#x9;&#x9;A.REGIONALITY = B.REGIONALITY &#xA;&#xA;&#x9;INSERT INTO dbo.FACT_SALES_REVENUE (AIRLINE_ID, DATE_ID, REVENUE_ID, REVENUE_AMT, PRODUCT_ID, EDW_INSERT_SOURCE, EDW_UPDATE_SOURCE, REGIONALITY)&#xA;&#x9;SELECT&#x9;AIRLINE_ID, DATE_ID, REVENUE_ID, REVENUE_AMT , PRODUCT_ID, EDW_INSERT_SOURCE, EDW_UPDATE_SOURCE, REGIONALITY--, SUM(REVENUE_AMT)&#xA;&#x9;FROM&#x9;#FSR_TEMP&#xA;--&#x9;GROUP&#x9;BY AIRLINE_ID, DATE_ID, REVENUE_ID, PRODUCT_ID, EDW_INSERT_SOURCE, EDW_UPDATE_SOURCE, REGIONALITY&#xA;&#x9;&#xA;&#x9;DELETE FROM #FSR_TEMP&#xA;&#xA;--PRINT 'DELETE FROM TEMP' &#xA;    DELETE FROM ##FACT_SALES_REVENUE WHERE IDC = @IDC &#xA;&#xA;    SET @IDC = @IDC + 1 &#xA;    SELECT @CNT = COUNT(1) FROM ##FACT_SALES_REVENUE &#xA;&#x9;IF @CNT = 0 &#xA;&#x9;BEGIN&#xA;&#x9;   SET @EXIT_FLAG = '1' &#xA;&#x9;END &#xA;END&#xA;" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
          </DTS:ObjectData>
        </DTS:Executable>
